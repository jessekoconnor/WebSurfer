{
  "cli": {
    "version": ">= 3.12.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "production": {
      "node": "16.20.0",
      "channel": "default",
      "env": {
        "APP_ENV": "production"
      }
    },
    "development": {
      "extends": "production",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "extends": "production",
      "distribution": "internal"
    },
    "testflight": {
      "extends": "production",
      "distribution": "store"
    }
  },
  "submit": {
    "production": {
      "ios": {
